<mvc:View controllerName="com.managegoods.controller.Main" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core">
    <Page title="Goods Status">
        <content>
            <!-- Shipping Goods --><VBox>
            <HBox alignItems="Center" justifyContent="SpaceBetween" alignContent="Center">
                <core:Icon src="sap-icon://shipping-status" size="5rem" />
                <Text text="Shipping Goods" />
            </HBox>
            
            <!-- Ready to Import Goods -->
            <HBox alignItems="centre" justifyContent="SpaceBetween" alignContent="Center">
                <core:Icon src="sap-icon://lab" size="5rem" />
                <Text text="Ready to Import Goods" />
            </HBox></VBox>
            <VBox>
            <!-- Not Available Goods -->
            <HBox alignItems="Center" justifyContent="SpaceBetween" alignContent="Center">
                <core:Icon src="sap-icon://status-negative" size="5rem" />
                <Text text="Not Available Goods" />
            </HBox>




            </VBox>
        </content>
    </Page>
</mvc:View>


